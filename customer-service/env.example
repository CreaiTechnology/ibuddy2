# Customer Service环境变量

# 服务器配置
PORT=3003
NODE_ENV=development

# 认证
JWT_SECRET=ibuddy2-customer-service-dev-secret
JWT_EXPIRY=24h

# 数据库配置
SUPABASE_URL=http://localhost:54321
SUPABASE_SERVICE_KEY=your-supabase-service-key

# MongoDB配置（用于更复杂的客户数据结构）
MONGODB_URI=mongodb://localhost:27017/ibuddy2-customers

# 消息队列
ENABLE_MESSAGE_QUEUE=false
RABBITMQ_URL=amqp://localhost:5672
RABBITMQ_QUEUE_NAME=customer-service-queue

# Redis缓存
ENABLE_REDIS_CACHE=false
REDIS_URL=redis://localhost:6379

# 核心服务配置
CORE_SERVICE_URL=http://localhost:3001

# AI服务配置
AI_SERVICE_URL=http://localhost:3002

# 日志
LOG_LEVEL=debug

# 速率限制
RATE_LIMIT_WINDOW_MS=900000  # 15分钟
RATE_LIMIT_MAX=100

# CORS设置
CORS_ORIGIN=http://localhost:8080 